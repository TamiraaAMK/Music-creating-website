<%- include('partials/header') %>

<h2>Top Rated Music</h2>

<% if (music.length > 0) { %>
    <ul>
        <% music.forEach(song => { %>
            <li><a href="/music/<%= song._id %>"><%= song.title %> by <%= song.artist %></a> - Rating: <%= song.averageRating %></li>
        <% }) %>
    </ul>
<% } else { %>
    <p>No top-rated music available.</p>
<% } %>

<%- include('partials/footer') %>

